{
  "extends": "expo/tsconfig.base",
  "include": ["./App.tsx", "./src/**/*.ts", "./src/**/*.tsx"],
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "jsx": "react-native",
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "@/*": ["./src/*"]
    }
  }
}
